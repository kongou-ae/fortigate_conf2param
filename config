    edit 14
        set srcintf "VLAN-2"
        set dstintf "wan1"
            set srcaddr "192.168.2.0/24" "192.168.1.0/24"             
            set dstaddr "all"             
        set action accept
        set utm-status enable
        set identity-based enable
        set nat enable
            config identity-based-policy
                edit 1
                    set schedule "always"
                    set logtraffic enable
                        set groups "FSSO_Guest_Users"                         
                        set service "ANY"                         
                next
            end
    next
