config firewall policy
    edit 9
        set srcintf "ssl.root"
        set dstintf "VLAN-2"
            set srcaddr "sslall_host"             
            set dstaddr "192.168.2.0/24"             
        set action accept
        set schedule "always"
            set service "TELNET" "ICMP_ANY"             
        set logtraffic enable
    next
    edit 10
        set srcintf "ssl.root"
